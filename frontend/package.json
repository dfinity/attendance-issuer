{
  "name": "early-adopter-campaign",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro check && astro build --mode development",
    "build:production": "astro check && astro build --mode production",
    "preview": "astro preview",
    "astro": "astro",
    "lint": "npm run lint:prettier && npm run lint:eslint",
    "lint:eslint": "eslint --ext .astro,.js,.ts . --max-warnings 0",
    "lint:prettier": "prettier --check .",
    "lint:fix": "npm run prettier:fix && npm run eslint:fix",
    "prettier:fix": "prettier --write .",
    "eslint:fix": "eslint --fix"
  },
  "dependencies": {
    "@astrojs/check": "^0.5.4",
    "@dfinity/auth-client": "^1.0.1",
    "astro": "^4.4.0",
    "typescript": "^5.3.3"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "eslint": "^8.56.0",
    "eslint-plugin-astro": "^0.31.4",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "prettier": "^3.2.5",
    "prettier-plugin-astro": "^0.13.0"
  }
}
